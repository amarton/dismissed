		meta(property="og:site_name", content="The Baltimore Sun")
		meta(property="og:description", content="Tenants lose, landlords win in Baltimore rent court")
		meta(property="fb:app_id", content="310302989040998")

		//Twitter 
		meta(name="twitter:card", content="summary")
		meta(name="twitter:site", content="@baltimoresun")
		meta(name="twitter:creator", content="@baltimoresun")
		meta(name="twitter:title", content="Dismissed")
		meta(name="twitter:description", content="Tenants lose, landlords win in Baltimore rent court")
